    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-react-auto-binding": "^1.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-pug-html": "^0.1.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
